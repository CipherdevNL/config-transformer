{
    "name": "symplify\/easy-testing",
    "description": "Testing made easy",
    "license": "MIT",
    "type": "symfony-bundle",
    "bin": [
        "bin\/easy-testing"
    ],
    "require": {
        "php": ">=7.3",
        "nette\/utils": "^3.2",
        "symfony\/finder": "^4.4|^5.3",
        "symfony\/http-kernel": "^4.4|^5.3",
        "symfony\/console": "^4.4|^5.3",
        "symfony\/dependency-injection": "^5.3",
        "symplify\/package-builder": "dev-main",
        "symplify\/console-package-builder": "dev-main",
        "symplify\/smart-file-system": "dev-main",
        "symplify\/symplify-kernel": "dev-main"
    },
    "require-dev": {
        "phpunit\/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ConfigTransformer202106125\\Symplify\\EasyTesting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConfigTransformer202106125\\Symplify\\EasyTesting\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}