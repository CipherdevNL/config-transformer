{
    "name": "symplify\/symplify-kernel",
    "description": "Internal Kernel for Symplify packages",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "symfony\/console": "^5.3.x-dev",
        "symfony\/http-kernel": "^5.3",
        "symfony\/dependency-injection": "^5.3.x-dev",
        "symplify\/smart-file-system": "dev-main",
        "symplify\/composer-json-manipulator": "dev-main",
        "symplify\/autowire-array-parameter": "dev-main",
        "symplify\/package-builder": "dev-main"
    },
    "require-dev": {
        "phpunit\/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ConfigTransformer202107130\\Symplify\\SymplifyKernel\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}