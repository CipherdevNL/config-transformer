{
    "name": "symplify\/symplify-kernel",
    "description": "Internal Kernel for Symplify packages",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "symfony\/console": "^4.4|^5.2",
        "symfony\/http-kernel": "^4.4|^5.2",
        "symfony\/dependency-injection": "^5.2",
        "symplify\/smart-file-system": "dev-main",
        "symplify\/composer-json-manipulator": "dev-main",
        "symplify\/autowire-array-parameter": "dev-main",
        "symplify\/package-builder": "dev-main"
    },
    "require-dev": {
        "phpunit\/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ConfigTransformer20210606\\Symplify\\SymplifyKernel\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}