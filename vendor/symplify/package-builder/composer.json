{
    "name": "symplify\/package-builder",
    "description": "Dependency Injection, Console and Kernel toolkit for Symplify packages.",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "nette\/utils": "^3.2",
        "symfony\/config": "^5.3",
        "symfony\/console": "^5.3",
        "symfony\/dependency-injection": "^5.3",
        "symfony\/finder": "^5.3",
        "symfony\/http-kernel": "^5.3",
        "symplify\/symplify-kernel": "dev-main",
        "symplify\/easy-testing": "dev-main",
        "nette\/neon": "^3.2"
    },
    "require-dev": {
        "phpunit\/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ConfigTransformer202107038\\Symplify\\PackageBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConfigTransformer202107038\\Symplify\\PackageBuilder\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}